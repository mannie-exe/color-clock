{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "inputs": ["./src/**/*.ts"],
      "outputs": ["./dist/*"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "inputs": ["./src/**/*.ts"],
      "outputs": ["./dist/*"],
      "dependsOn": ["^build"],
      "cache": false
    }
  }
}
